<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Page Title</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" type="text/css" media="screen" href="/stylesheets/style.css" />
  <script src="main.js"></script>
</head>
<body>
  <div class='header_section'>
    <h1>BLOG POST</h1>
    <a href="/blog"<button class="btn-create">create</button></a>
  </div>
  <div class="display-info">
    <% blogs.forEach(function(blog){ %>
      <div class="one-card">
        <a href="/detail/<%= blog.id %>"> <h4><%= blog.title %></h4></a>
        <p><%= blog.description %></p>
        <h5><%= blog.date %></h5>
        <a href="/blog/<%= blog.id %>"><button class="remove button-style">remove</button></a>
        <a href="/edit/<%= blog.id %>/edit"><button class="edit button-style">edit</button></a>
    </div>
      <% }); %> 
  </div>
</body>
</html>